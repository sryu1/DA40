	<!--Master Key-->
	<animation>
		<type>pick</type>
		<object-name>key.g1000</object-name>
		<object-name>master.inner.g1000</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>property-cycle</command>
				<property>controls/electric/electric-master</property>
				<value>1</value>
				<value>2</value>
				<value>0</value>
			</binding>
			<mod-up>
				<binding>
					<condition>
						<equals>
							<property>controls/electric/electric-master</property>
							<value>2</value>
						</equals>
					</condition>
					<command>property-assign</command>
					<property>controls/electric/electric-master</property>
					<value>1</value>
				</binding>
			</mod-up>
		</action>
		<action>
			<button>1</button>
			<binding>
				<command>property-cycle</command>
				<property>controls/electric/electric-master</property>
				<value>0</value>
				<value>2</value>
				<value>1</value>
			</binding>
			<mod-up>
				<binding>
					<condition>
						<equals>
							<property>controls/electric/electric-master</property>
							<value>2</value>
						</equals>
					</condition>
					<command>property-assign</command>
					<property>controls/electric/electric-master</property>
					<value>1</value>
				</binding>
			</mod-up>
		</action>
