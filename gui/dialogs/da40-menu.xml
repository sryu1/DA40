<?xml version="1.0"?>

<PropertyList>
	<default>
		<menu n="10">
			<label>Diamond DA-40 NG</label>
			<enabled type="bool">true</enabled>

			<item>
				<label>Aircraft Options</label>
				<binding>
					<command>dialog-show</command>
					<dialog-name>aircraft-dialog</dialog-name>
				</binding>
			</item>

			<item>
				<label>Autostart</label>
				<binding>
					<command>nasal</command>
					<script>autostart.autostart();</script>
				</binding>
			</item>
			<item>
				<label>Select Livery</label>
				<binding>
					<command>nasal</command>
					<script>aircraft.livery.dialog.open()</script>
				</binding>
			</item>
            <item>
                <label>Panel Configuration</label>
                <binding>
                    <command>dialog-show</command>
                    <dialog-name>da40_panel_config</dialog-name>
                </binding>
            </item>
		</menu>

	</default>

</PropertyList>